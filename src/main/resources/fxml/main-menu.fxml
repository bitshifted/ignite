<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="mainMenuBar" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="co.bitshifted.appforge.ignite.ctrl.MainMenuController">
  <menus>
    <Menu fx:id="fileMenu" text="%menu.main.file">
      <items>
        <MenuItem fx:id="newDeploymentMenuItem" mnemonicParsing="false" onAction="#newDeploymentAction" text="%menu.main.file.newdeployment" />
      </items>
    </Menu>
  </menus>
</MenuBar>
