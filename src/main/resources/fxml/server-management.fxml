<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ListView fx:id="serverListView" prefHeight="100.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </ListView>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="55.0" prefWidth="600.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="newServerButton" mnemonicParsing="false" onAction="#onNewServerAction" text="%server.mgmt.button.new" />
            <Button fx:id="editServerButton" disable="true" mnemonicParsing="false" onAction="#onEditServerAction" text="%server.mgmt.button.edit" />
            <Button fx:id="deleteServerButton" disable="true" mnemonicParsing="false" onAction="#onDeleteServerAction" text="%server.mgmt.button.delete" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
